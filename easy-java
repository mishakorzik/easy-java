        if [ -d "$HOME/.oh-my-zsh" ]; then
            if grep -q "java-20()" "$HOME/.zshrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-20"
            else
                echo 'java-20() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-20; java $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo 'javac-20() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-20; javac $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-20"
            fi
        else
            if grep -q "java-20()" "$PREFIX/etc/bash.bashrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-20"
            else
                echo 'java-20() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-20; java $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo 'javac-20() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-20; javac $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-20"
            fi
        fi
    elif [ "$option" -eq 4 ]; then
        cd /data/data/com.termux/files/java/usr/local
        rm -rf jdk-19
        echo "Downloading Oracle Java 19"
        wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-19-latest-aarch64_bin.tar.gz
        echo "Extracting Oracle Java 19"
        tar -xf jdk-19-latest-aarch64_bin.tar.gz
        rm jdk-19-latest-aarch64_bin.tar.gz
        echo 'alias java-19="export PATH=/usr/local/jdk-19/bin:$PATH"' >> /data/data/com.termux/files/java/root/.bashrc
        if [ -d "$HOME/.oh-my-zsh" ]; then
            if grep -q "java-19()" "$HOME/.zshrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
