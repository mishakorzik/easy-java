            else
                echo 'java-19() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-19; java $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo 'javac-19() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-19; javac $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-19"
            fi
        fi
    elif [ "$option" -eq 5 ]; then
        cd /data/data/com.termux/files/java/usr/local
        rm -rf jdk-17
        echo "Downloading Oracle Java 17"
        wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-17-latest-aarch64_bin.tar.gz
