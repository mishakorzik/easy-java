            fi
        elif [ "$option" -eq 6 ]; then
            cd /usr/local
            sudo rm -rf jdk-16
            echo "Downloading Oracle Java 16"
            sudo wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-15-latest-aarch64_bin.tar.gz
            echo "Extracting Oracle Java 16"
            sudo tar -xf jdk-16-latest-aarch64_bin.tar.gz
            sudo rm jdk-16-latest-aarch64_bin.tar.gz
            if [ -d "$HOME/.oh-my-zsh" ]; then
                if grep -q "java-16()" "$HOME/.zshrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-16"
                else
                    echo 'java-16() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-16/bin:$PATH; java $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo 'javac-16() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-16/bin:$PATH; javac $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-16"
                fi
            else
                if grep -q "java-16()" "$HOME/.bashrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-16"
                else
                    echo 'java-16() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-16/bin:$PATH; java $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo 'javac-16() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-16/bin:$PATH; javac $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-16"
                fi
            fi
        elif [ "$option" -eq 7 ]; then
            cd /usr/local
            sudo rm -rf jdk-15
            echo "Downloading Oracle Java 15"
            sudo wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-15-latest-aarch64_bin.tar.gz
            echo "Extracting Oracle Java 15"
            sudo tar -xf jdk-15-latest-aarch64_bin.tar.gz
            sudo rm jdk-15-latest-aarch64_bin.tar.gz
            if [ -d "$HOME/.oh-my-zsh" ]; then
                if grep -q "java-15()" "$HOME/.zshrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-15"
                else
                    echo 'java-15() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-15/bin:$PATH; java $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo 'javac-15() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-15/bin:$PATH; javac $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-15"
                fi
            else
                if grep -q "java-15()" "$HOME/.bashrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-15"
                else
                    echo 'java-15() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-15/bin:$PATH; java $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo 'javac-15() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-15/bin:$PATH; javac $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-15"
                fi
            fi
        elif [ "$option" -eq 8 ]; then
            cd /usr/local
            sudo rm -rf jdk-11
            echo "Downloading Oracle Java 11"
            sudo wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-11-latest-aarch64_bin.tar.gz
            echo "Extracting Oracle Java 11"
            sudo tar -xf jdk-11-latest-aarch64_bin.tar.gz
            sudo rm jdk-11-latest-aarch64_bin.tar.gz
            if [ -d "$HOME/.oh-my-zsh" ]; then
                if grep -q "java-11()" "$HOME/.zshrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-11"
                else
                    echo 'java-11() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-11/bin:$PATH; java $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo 'javac-11() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-11/bin:$PATH; javac $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-11"
                fi
            else
                if grep -q "java-11()" "$HOME/.bashrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-11"
                else
                    echo 'java-11() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-11/bin:$PATH; java $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo 'javac-11() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-11/bin:$PATH; javac $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-11"
                fi
            fi
        elif [ "$option" -eq 9 ]; then
            cd /usr/local
            sudo rm -rf jdk1.8.0
            echo "Downloading Oracle Java 8"
            sudo wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-8-latest-aarch64_bin.tar.gz
            echo "Extracting Oracle Java 8"
            sudo tar -xf jdk-8-latest-aarch64_bin.tar.gz
            sudo rm jdk-8-latest-aarch64_bin.tar.gz
            if [ -d "$HOME/.oh-my-zsh" ]; then
                if grep -q "java-8()" "$HOME/.zshrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-8"
                else
                    echo 'java-8() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk-8/bin:$PATH; java $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo 'javac-8() {' >> $HOME/.zshrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk-8/bin:$PATH; javac $*' >> $HOME/.zshrc
                    echo '}' >> $HOME/.zshrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-8"
                fi
            else
                if grep -q "java-8()" "$HOME/.bashrc"; then
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-8"
                else
                    echo 'java-8() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjava args\033[0m: $*"; export PATH=/usr/local/jdk1.8.0/bin:$PATH; java $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo 'javac-8() {' >> $HOME/.bashrc
                    echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; export PATH=/usr/local/jdk1.8.0/bin:$PATH; javac $*' >> $HOME/.bashrc
                    echo '}' >> $HOME/.bashrc
                    echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                    echo -e "To run java, restart your shell and write in terminal: java-8"
                fi
            fi
        elif [ "$option" -eq 10 ]; then
            exit 1
        else
            echo "Invalid option"
        exit 1
        fi
    else
        echo "This system is not supported. Supported systems: debian, ubuntu, termux."
        exit 1
    fi
fi

