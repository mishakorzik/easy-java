#!/usr/bin/bash
if [ -d "$PREFIX" ] && [ "$PREFIX" = "/data/data/com.termux/files/usr" ]; then
    if [ "$(uname -m)" != "aarch64" ]; then
        echo "This system is not arm64 architecture."
        exit 1
    fi
    if command -v proot &> /dev/null
    then
        echo -e "proot is \033[01;32minstalled\033[0m"
    else
        echo -e "proot is \033[01;31mnot\033[0m installed"
        echo -e "installing proot"
        pkg install proot -y
    fi
    if command -v debootstrap &> /dev/null
    then
        echo -e "debootstrap is \033[01;32minstalled\033[0m"
    else
        echo -e "debootstrap is \033[01;31mnot\033[0m installed"
        echo -e "installing debootstrap"
        pkg install debootstrap -y
    fi
    if command -v wget &> /dev/null
    then
        echo -e "wget is \033[01;32minstalled\033[0m"
    else
        echo -e "wget is \033[01;31mnot\033[0m installed"
        echo -e "installing wget"
        pkg install wget -y
    fi
    if command -v tar &> /dev/null
    then
        echo -e "tar is \033[01;32minstalled\033[0m"
    else
        echo -e "tar is \033[01;31mnot\033[0m installed"
        echo -e "installing tar"
        pkg install tar -y
    fi
    echo -e ""
    echo -e "\033[01;34m01\033[0m. Oracle Java 22"
    echo -e "\033[01;34m02\033[0m. Oracle Java 21"
    echo -e "\033[01;34m03\033[0m. Oracle Java 20"
    echo -e "\033[01;34m04\033[0m. Oracle Java 19"
    echo -e "\033[01;34m05\033[0m. Oracle Java 17"
    echo -e "\033[01;34m06\033[0m. Oracle Java 16"
    echo -e "\033[01;34m07\033[0m. Oracle Java 15"
    echo -e "\033[01;34m08\033[0m. Oracle Java 11"
    echo -e "\033[01;34m09\033[0m. Oracle Java 8"
    echo -e "\033[01;34m10\033[0m. Quit"

    read -p "Choose an option: " option
    if [ -d "$PREFIX/../java" ]; then
        echo "skipping debian container installing"
    else
        echo "installing debian container. (It will take a long time)"
        debootstrap --variant=minbase --arch=arm64 --components=main stable $PREFIX/../java http://ftp.debian.org/debian/ > /dev/null 2>&1
        echo "verifying debian container"
        unset LD_PRELOAD
        if proot -0 --link2symlink -r $PREFIX/../java -w /root -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin grep -q "ID=debian" /etc/os-release; then
            echo -e "debian container \033[01;32minstalled\033[0m"
        else
            rm -rf $PREFIX/../java
            echo -e "\033[01;31mfailed\033[0m"
            exit 1
        fi
    fi
    if [ "$option" -eq 1 ]; then
        cd /data/data/com.termux/files/java/usr/local
        rm -rf jdk-22
        echo "Downloading Oracle Java 22"
        wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-22-latest-aarch64_bin.tar.gz
        echo "Extracting Oracle Java 22"
        tar -xf jdk-22-latest-aarch64_bin.tar.gz
        rm jdk-22-latest-aarch64_bin.tar.gz
        echo 'alias java-22="export PATH=/usr/local/jdk-22/bin:$PATH"' >> /data/data/com.termux/files/java/root/.bashrc
        if [ -d "$HOME/.oh-my-zsh" ]; then
            if grep -q "java-22()" "$HOME/.zshrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-22"
            else
                echo 'java-22() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-22; java $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo 'javac-22() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-22; javac $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-22"
            fi
        else
            if grep -q "java-22()" "$PREFIX/etc/bash.bashrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-22"
            else
                echo 'java-22() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-22; java $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo 'javac-22() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-22; javac $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-22"
            fi
        fi
    elif [ "$option" -eq 2 ]; then
        cd /data/data/com.termux/files/java/usr/local
        rm -rf jdk-21
        echo "Downloading Oracle Java 21"
        wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-21-latest-aarch64_bin.tar.gz
        echo "Extracting Oracle Java 21"
        tar -xf jdk-21-latest-aarch64_bin.tar.gz
        rm jdk-21-latest-aarch64_bin.tar.gz
        echo 'alias java-21="export PATH=/usr/local/jdk-21/bin:$PATH"' >> /data/data/com.termux/files/java/root/.bashrc
        if [ -d "$HOME/.oh-my-zsh" ]; then
            if grep -q "java-21()" "$HOME/.zshrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-21"
            else
                echo 'java-21() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-21; java $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo 'javac-21() {' >> $HOME/.zshrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-21; javac $*"' >> $HOME/.zshrc
                echo '}' >> $HOME/.zshrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-21"
            fi
        else
            if grep -q "java-21()" "$PREFIX/etc/bash.bashrc"; then
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-21"
            else
                echo 'java-21() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-21; java $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo 'javac-21() {' >> $PREFIX/etc/bash.bashrc
                echo '  echo -e "\033[01;34mjavac args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-21; javac $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo -e "Installation completed \033[01;32msuccessfully\033[0m"
                echo -e "To run java, restart your shell and write in terminal: java-21"
            fi
        fi
    elif [ "$option" -eq 3 ]; then
        cd /data/data/com.termux/files/java/usr/local
        rm -rf jdk-20
        echo "Downloading Oracle Java 20"
        wget -q --show-progress https://github.com/mishakorzik/easy-java/releases/download/Java/jdk-20-latest-aarch64_bin.tar.gz
