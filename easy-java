                echo '  echo -e "\033[01;34mjava args\033[0m: $*"; unset LD_PRELOAD; proot -0 --link2symlink -r $PREFIX/../java -w "$PWD" -b /dev/ -b /sys/ -b /proc/ -b /data/data/com.termux/files/home /usr/bin/env -i HOME=/root TERM="xterm-256color" PATH=/bin:/usr/bin:/sbin:/usr/sbin /bin/bash -ic "java-16; java $*"' >> $PREFIX/etc/bash.bashrc
                echo '}' >> $PREFIX/etc/bash.bashrc
                echo 'javac-16() {' >> $PREFIX/etc/bash.bashrc
